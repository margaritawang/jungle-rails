<h1>Log In!</h1>

<%= form_for :sessions, url:'/sessions'  do |form| %>

  <div class="form-group">
    <%= form.label :email %>
    <%= text_field_tag :email %>
  </div>

  <div class="form-group">
  <%= form.label :password %>
  <%= password_field_tag :password %>
  </div>

  <%= form.submit "Submit" %>

<% end %>